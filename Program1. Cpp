#include <iostream>
using namespace std;

class Student {
public:
    inline void displaySum(int a, int b) {
        cout << "Sum: " << (a + b) << endl;
    }
};

int main() {
    Student s;
    int num1, num2;

    cout << "Enter two numbers: ";
    cin >> num1 >> num2;

    s.displaySum(num1, num2);

    return 0;
}
